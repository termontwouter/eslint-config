{
  "name": "@rubensworks/eslint-config",
  "version": "3.0.0",
  "description": "Personal eslint config",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "repository": "git@github.com:rubensworks/eslint-config.git",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "bugs": {
    "url": "https://github.com/rubensworks/eslint-config/issues"
  },
  "homepage": "https://github.com/rubensworks/eslint-config#readme",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "resolutions": {
    "@eslint/eslintrc@*": "https://github.com/eslint/eslintrc"
  },
  "dependencies": {
    "@eslint/eslintrc": "^2.1.3",
    "@eslint/js": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint-import-resolver-typescript": "^3.0.0",
    "eslint-plugin-eslint-comments": "^3.0.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-mocha": "^10.0.5",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unicorn": "^48.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "globals": "^13.23.0",
    "typescript": "^5.0.0 <5.3.0",
    "@types/eslint": "^8.0.0",
    "@types/eslint__eslintrc": "^2.1.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.10.0"
  },
  "devDependencies": {
    "eslint": "^8.35.0",
    "manual-git-changelog": "^1.0.2",
    "pre-commit": "^1.2.2"
  },
  "peerDependencies": {
    "eslint": "^8.35.0"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": false
    }
  },
  "scripts": {
    "build": "yarn exec tsc",
    "test": "echo \"NOOP\"",
    "lint": "yarn build && yarn run eslint .",
    "version": "yarn exec manual-git-changelog onversion",
    "postinstall": "yarn build"
  },
  "pre-commit": [
    "lint"
  ]
}
