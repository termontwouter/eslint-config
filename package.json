{
  "name": "@rubensworks/eslint-config",
  "version": "3.0.0",
  "description": "Personal eslint config",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "packageManager": "yarn@4.0.2",
  "engines": {
    "node": ">=18.18"
  },
  "type": "module",
  "main": "./dist/index.js",
  "repository": "git@github.com:rubensworks/eslint-config.git",
  "author": "Ruben Taelman <rubensworks@gmail.com>",
  "bugs": {
    "url": "https://github.com/rubensworks/eslint-config/issues"
  },
  "homepage": "https://github.com/rubensworks/eslint-config#readme",
  "license": "MIT",
  "files": [
    "dist",
    "package.json",
    ".yarn/patches"
  ],
  "scripts": {
    "build": "yarn exec tsc",
    "test": "echo \"NOOP\"",
    "lint": "yarn build && yarn run eslint .",
    "version": "yarn exec manual-git-changelog onversion",
    "postinstall": "yarn build"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.0.1",
    "@eslint/js": "^9.0.0-beta.1",
    "@stylistic/eslint-plugin": "^1.5.1",
    "@types/eslint": "^8.56.3",
    "@types/eslint__eslintrc": "^2.1.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.1",
    "@typescript-eslint/parser": "^7.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-eslint-comments": "^3.2",
    "eslint-plugin-import": "^2.29",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unicorn": "^51",
    "eslint-plugin-unused-imports": "^3.1",
    "espree": "^9.6.1",
    "globals": "^13.23.0",
    "typescript": "^5.0.0 <5.4.0"
  },
  "devDependencies": {
    "eslint": "^9.0.0-beta.1",
    "manual-git-changelog": "^1.0.2",
    "pre-commit": "^1.2.2"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0"
  },
  "peerDependenciesMeta": {
    "eslint": {
      "optional": false
    }
  },
  "pre-commit": [
    "lint"
  ]
}
